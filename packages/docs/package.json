{
	"name": "@promptly/docs",
	"version": "0.0.1",
	"private": true,
	"description": "Promptly - Docs",
	"keywords": [
		"promptly",
		"docs"
	],
	"homepage": "https://hieudoanm.github.io/promptly",
	"bugs": {
		"url": "https://github.com/hieudoanm/promptly/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/hieudoanm/promptly.git"
	},
	"license": "GPL-3.0",
	"author": "hieudoanm <hieumdoan@gmail.com> (https://hieudoanm.github.io)",
	"packageManager": "pnpm@10.28.0",
	"engines": {
		"node": "24.13.0"
	},
	"scripts": {
		"docusaurus": "docusaurus",
		"start": "docusaurus start",
		"prebuild": "rimraf ../../docs .docusaurus",
		"build": "docusaurus build --out-dir ../../docs",
		"postbuild": "touch ../../docs/.nojekyll",
		"swizzle": "docusaurus swizzle",
		"deploy": "docusaurus deploy",
		"clear": "docusaurus clear",
		"serve": "docusaurus serve",
		"write-translations": "docusaurus write-translations",
		"write-heading-ids": "docusaurus write-heading-ids",
		"typecheck": "tsc",
		"format": "prettier --cache --write ."
	},
	"dependencies": {
		"@docusaurus/core": "3.9.2",
		"@docusaurus/preset-classic": "3.9.2",
		"@docusaurus/theme-classic": "3.9.2",
		"@mdx-js/react": "3.1.1",
		"clsx": "2.1.1",
		"prism-react-renderer": "2.4.1",
		"react": "19.2.3",
		"react-dom": "19.2.3"
	},
	"devDependencies": {
		"@docusaurus/eslint-plugin": "3.9.2",
		"@docusaurus/module-type-aliases": "3.9.2",
		"@docusaurus/tsconfig": "3.9.2",
		"@docusaurus/types": "3.9.2",
		"@eslint/js": "9.39.2",
		"eslint": "9.39.2",
		"eslint-plugin-react": "7.37.5",
		"globals": "17.0.0",
		"prettier": "3.8.0",
		"rimraf": "6.1.2",
		"typescript": "~5.9.3",
		"typescript-eslint": "8.53.0"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 3 chrome version",
			"last 3 firefox version",
			"last 5 safari version"
		]
	}
}
